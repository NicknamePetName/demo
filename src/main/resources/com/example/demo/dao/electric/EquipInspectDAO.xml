<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.electric.EquipInspectDAO">

    <resultMap id="equipInspectResultMap" type="com.example.demo.dataobject.electric.EquipInspectDO">
        <id column="id" property="id"/>
        <result column="tester_num" property="testerNum"/>
        <result column="power_on_num" property="powerOnNum"/>
        <result column="pro_loop_num" property="proLoopNum"/>
        <result column="drawer_confirm" property="drawerConfirm"/>
        <result column="process_confirm" property="processConfirm"/>
        <result column="transfer_list_confirm" property="transferListConfirm"/>
        <result column="gmt_created" property="gmtCreated"/>
        <result column="gmt_modified" property="gmtModified"/>
    </resultMap>

    <insert id="insert" parameterType="com.example.demo.dataobject.electric.EquipInspectDO" useGeneratedKeys="true" keyColumn="id" keyProperty="id">
        INSERT INTO equip_inspect(tester_num,power_on_num,pro_loop_num,drawer_confirm,process_confirm,transfer_list_confirm,gmt_created,gmt_modified)
        VALUES (#{testerNum},#{powerOnNum},#{proLoopNum},#{drawerConfirm},#{processConfirm},#{transferListConfirm},now(),now())
    </insert>



</mapper>