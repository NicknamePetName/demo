<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.electric.ElectricInspectDAO">

    <resultMap id="electricInspectResultMap" type="com.example.demo.dataobject.electric.ElectricInspectDO">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="table_code" property="tableCode"/>
        <result column="project_name" property="projectName"/>
        <result column="cabinet_type" property="cabinetType"/>
        <result column="cabinet_num" property="cabinetNum"/>
        <result column="inspector" property="inspector"/>
        <result column="date" property="date"/>
        <result column="other" property="other"/>
        <result column="remark" property="remark"/>
        <result column="equip_inspect_id" property="equipInspectId"/>
        <result column="loop_inspect_id" property="loopInspectId"/>
        <result column="inspect_item_id" property="inspectItemId"/>
        <result column="other_no_regular_id" property="otherNoRegularId"/>
        <result column="gmt_created" property="gmtCreated"/>
        <result column="gmt_modified" property="gmtModified"/>
    </resultMap>

    <insert id="insert" parameterType="com.example.demo.dataobject.electric.ElectricInspectDO" useGeneratedKeys="true" keyColumn="id" keyProperty="id">
        INSERT INTO electric_inspect(user_id,table_code,project_name,cabinet_type,cabinet_num,inspector,date,other,
                         remark,equip_inspect_id,loop_inspect_id,inspect_item_id,
                         other_no_regular_id,gmt_created,gmt_modified)
        VALUES (#{userId},#{tableCode},#{projectName},#{cabinetType},#{cabinetNum},#{inspector},#{date},
                #{other},#{remark},#{equipInspectId},#{loopInspectId},#{inspectItemId},
                #{otherNoRegularId},now(),now())
    </insert>


</mapper>